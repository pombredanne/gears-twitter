module.exports = {
	onAdd: function (d, v) {
		v.rm('stackato.yml');
		v.rm('.csslintrc');
		v.rm('.jshintrc');
		v.rm('.jshintrc');
		v.rm('Gruntfile.js');
		v.rm('LICENSE-MIT');
		v.rm('README.md');
		v.rm('app.js');
		v.rm('lib');
		v.rm('node_modules');
		v.rm('package.json');
		v.rm('src');
		v.rm('views');
		v.copyDir('dist', './');
		v.rm('dist');
		v.rm('volofile');
		d.resolve();
	}
};
